-- Creating tables for Rock
CREATE TABLE rock (
	index int NOT NULL,
     track_id VARCHAR(30) NOT NULL,
     track_name VARCHAR(140) NOT NULL,
	 artist_id VARCHAR NOT NULL,
	artist_name VARCHAR NOT NULL,
	 album_id VARCHAR NOT NULL,
	 duration int NOT NULL,
	 release_date VARCHAR NOT NULL,
	 popularity int NOT NULL,
	 danceability DECIMAL NOT NULL,
	energy DECIMAL NOT NULL,
	key int NOT NULL,
	loudness DECIMAL NOT NULL,
	mode int NOT NULL,
	speechiness DECIMAL NOT NULL,
	acousticness DECIMAL NOT NULL,
	instrumentalness DECIMAL NOT NULL,
	liveness DECIMAL NOT NULL,
	valence DECIMAL NOT NULL,
	tempo DECIMAL NOT NULL,
	playlist_id VARCHAR NOT NULL,
	 playlist_name VARCHAR NOT NULL,
	 Genre VARCHAR NOT NULL,
	 PRIMARY KEY (track_id)
);
-- Creating tables for Rap
CREATE TABLE rap (
     index int NOT NULL,
     track_id VARCHAR(30) NOT NULL,
     track_name VARCHAR(140) NOT NULL,
	 artist_id VARCHAR NOT NULL,
	artist_name VARCHAR NOT NULL,
	 album_id VARCHAR NOT NULL,
	 duration int NOT NULL,
	 release_date VARCHAR NOT NULL,
	 popularity int NOT NULL,
	 danceability DECIMAL NOT NULL,
	energy DECIMAL NOT NULL,
	key int NOT NULL,
	loudness DECIMAL NOT NULL,
	mode int NOT NULL,
	speechiness DECIMAL NOT NULL,
	acousticness DECIMAL NOT NULL,
	instrumentalness DECIMAL NOT NULL,
	liveness DECIMAL NOT NULL,
	valence DECIMAL NOT NULL,
	tempo DECIMAL NOT NULL,
	playlist_id VARCHAR NOT NULL,
	 playlist_name VARCHAR NOT NULL,
	 Genre VARCHAR NOT NULL,
	 PRIMARY KEY (track_id)
);
-- Creating tables for R&B
CREATE TABLE rb (
     index int NOT NULL,
